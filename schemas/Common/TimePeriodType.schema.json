{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "TimePeriodType.schema.json",
    "title": "TimePeriodType",
    "definitions": {
        "TimePeriodType": {
            "$id": "#TimePeriodType",
            "description": "Representation of a time period",
            "properties": {
                "endDateTime": {
                    "type": "string",
                    "format": "date-time",
                    "description": "End date of the period"
                },
                "startDateTime": {
                    "type": "string",
                    "format": "date-time",
                    "description": "Start date of the period"
                }
            },
            "required": [
                "startDateTime"
            ],
            "type": "object"
        }
    }
}

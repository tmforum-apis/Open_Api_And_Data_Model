{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "TaskStateType.schema.json",
    "title": "TaskStateType",
    "definitions": {
        "TaskStateType": {
            "$id": "#TaskStateType",
            "type": "string",
            "description": "Possible values for the state of a task",
            "enum": [
                "acknowledged",
                "terminatedWithError",
                "inProgress",
                "done"
            ]
        }
    }
}

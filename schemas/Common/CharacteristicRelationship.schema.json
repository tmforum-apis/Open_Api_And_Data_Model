{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "CharacteristicRelationship.schema.json",
  "title": "CharacteristicRelationship",
  "definitions": {
    "CharacteristicRelationship": {
      "$id": "#CharacteristicRelationship",
